{
  "name": "pulse-dashboards",
  "version": "0.3.0",
  "engines": {
    "node": "12.x.x"
  },
  "description": "Bringing criminal justice analysis to decision makers to help reduce incarceration",
  "private": true,
  "repository": "git@github.com:Recidiviz/pulse-dashboards.git",
  "author": "Recidiviz <team@recidiviz.org>",
  "license": "GNU General Public License v3",
  "scripts": {
    "start": "node server.js",
    "build": "react-scripts build",
    "build-staging": "env-cmd -f .env.development react-scripts build",
    "test-react": "TZ=UTC react-scripts test --env=jest-environment-jsdom-sixteen",
    "test-server": "TZ=UTC jest ./server",
    "test-filters": "TZ=UTC jest ./shared-filters",
    "test": "yarn test-server && yarn test-filters && yarn test-react",
    "spa": "react-scripts start",
    "server": "nodemon server.js",
    "server:dev": "npm-run-all --parallel server redis-server",
    "server:demo": "IS_DEMO=true yarn server:dev",
    "redis-server": "redis-server",
    "lint": "tsc && eslint '**/*.{js,ts,tsx}'",
    "dev": "npm-run-all --parallel spa server:dev",
    "demo": "npm-run-all --parallel spa server:demo"
  },
  "dependencies": {
