{
  "name": "pulse-dashboards",
  "version": "0.3.0",
  "engines": {
    "node": "16.x.x"
  },
  "description": "Bringing criminal justice analysis to decision makers to help reduce incarceration",
  "private": true,
  "repository": "git@github.com:Recidiviz/pulse-dashboards.git",
  "author": "Recidiviz <team@recidiviz.org>",
  "license": "GNU General Public License v3",
  "scripts": {
    "start": "node server.js",
    "build": "source ./load_environment.sh env_production && ./load_config_files.sh && react-scripts build",
    "build-staging": "source ./load_environment.sh env_staging && ./load_config_files.sh && react-scripts build",
    "build-demo": "source ./load_environment.sh env_staging_demo && ./load_config_files.sh && react-scripts build",
    "build-e2e": "env-cmd -f .env.offline react-scripts build",
    "test-e2e-lantern": "source ./load_environment.sh env_e2e && wdio ./wdio.conf.js --suite lantern --retry 2",
    "test-e2e-login": "source ./load_environment.sh env_e2e && wdio ./wdio.conf.js --suite login --retry 2",
    "test-e2e-users": "source ./load_environment.sh env_e2e && wdio ./wdio.conf.js --suite userAccess --retry 2",
    "test-e2e-workflows": "env-cmd -f .env.offline wdio ./wdio.conf.js --suite workflows --retry 2",
    "test-react": "TZ=UTC react-scripts test --resetMocks=false --verbose=false",
    "test-server": "TZ=UTC jest ./server --setupFilesAfterEnv ./server/setupTests.js --resetMocks=false --runInBand",
    "test-filters": "TZ=UTC jest ./shared-filters --resetMocks=false",
    "test-firestore": "TZ=UTC firebase emulators:exec --only firestore 'yarn jest ./firestore-config --env=node'",
    "test-zod": "source ./load_environment.sh env_dev_server && ts-node tools/testZodSchema.ts",
    "load-demo-fixtures": "source ./load_environment.sh env_dev_server && ts-node tools/loadWorkflowsFixtures.ts",
    "test": "yarn test-server && yarn test-filters && yarn test-react",
    "lint": "tsc && eslint '**/*.{js,ts,tsx}'",
    "redis-server": "redis-server --port 6380",
    "server": "nodemon server.js",
    "dev": "./load_config_files.sh && npm-run-all --parallel spa server:dev",
    "spa": "source ./load_environment.sh env_dev_spa && react-scripts start",
    "server:dev": "source ./load_environment.sh env_dev_server && npm-run-all --parallel server redis-server",
    "dev:be": "npm-run-all --parallel spa:be server:dev",
    "spa:be": "source ./load_environment.sh env_dev_spa,env_dev_be_spa && react-scripts start",
    "demo": "npm-run-all --parallel spa:demo server:demo",
    "spa:demo": "source ./load_environment.sh env_dev_spa,env_dev_demo_spa && react-scripts start",
    "server:demo": "source ./load_environment.sh env_dev_server,env_dev_demo_server && npm-run-all --parallel server redis-server",
    "offline": "env-cmd -f .env.offline npm-run-all --parallel spa:offline server redis-server firebase-offline",
    "spa:offline": "env-cmd -f .env.offline react-scripts start",
    "server:offline": "env-cmd -f .env.offline npm-run-all --parallel server redis-server firebase-offline",
    "offline:be": "npm-run-all --parallel pathways-backend offline",
    "pull-pathways-backend": "docker compose pull case_triage_backend",
    "pathways-backend": "sh tools/run_pathways_backend.sh",
    "sync-content": "source ./load_environment.sh env_sync_content && ts-node tools/syncContentFromSheets.ts && prettier --write src/core/content/*",
    "firebase-emulator": "firebase --project=demo-dev emulators:start --import firestore-config/emulator-fixtures",
    "update-workflows-fixture": "firebase emulators:exec --project=demo-dev --only firestore --export-on-exit firestore-config/emulator-fixtures 'NODE_OPTIONS=\"--unhandled-rejections=strict\" ts-node tools/loadWorkflowsFixtures.ts'",
    "firebase-offline": "run-s update-workflows-fixture firebase-emulator",
    "update-rules-staging": "firebase --project=staging-backend deploy --only firestore:rules",
    "update-rules-production": "firebase --project=production-backend deploy --only firestore:rules",
    "deploy": "node ./tools/deploy.mjs",
    "load-prod-fixtures": "ts-node tools/loadProdTestFixtures.mjs",
    "pdfformfiller-boilerplate": "ts-node tools/pdfFormFillerBoilerplate.mjs"
  },
