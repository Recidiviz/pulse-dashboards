{
  "extends": ["@recidiviz", "plugin:wdio/recommended"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        // TODO TS re-enable this after repo is ported to TS
        "@typescript-eslint/no-explicit-any": "off",
        // TODO TS re-enable this after repo is ported to TS
        "@typescript-eslint/ban-ts-comment": "off"
      }
    }
  ],
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error"] }]
  }
}
