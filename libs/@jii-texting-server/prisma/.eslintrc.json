{
  "extends": ["../../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        // This enforces the use of path aliases
        "no-restricted-imports": ["error", {
          "patterns": [".*"]
       }]
      }
    }
  ]
}
